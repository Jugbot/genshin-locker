{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "format:write",
          "format",
          "lint:write",
          "lint",
          "check-types",
          "compile",
          "build"
        ]
      }
    }
  },
  "targetDefaults": {
    "compile": {
      "dependsOn": [
        "^compile",
        "build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
